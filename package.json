{
	"name": "@ztimson/js-utilities",
	"version": "0.3.1",
	"description": "JavaScript Utility library",
	"author": "Zak Timson",
	"license": "MIT",
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://git.zakscode.com/ztimson/js-utilities"
	},
	"main": "./dist/js-utilities.js",
	"module": "./dist/js-utilities.mjs",
	"types": "./dist/src/index.d.ts",
	"scripts": {
