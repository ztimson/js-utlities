		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vite-plugin-dts": "^3.7.2"
	},
	"exports": {
		".": {
			"import": "./dist/js-utilities.mjs",
			"require": "./dist/js-utilities.js"
		}
	},
	"files": [
		"dist"
	]
